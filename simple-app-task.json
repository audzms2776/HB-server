{
  "taskDefinition": {
    "family": "console-sample-app-static",
    "containerDefinitions": [
      {
        "name": "simple-app",
        "image": "npmmmdocker/hbnode:init",
        "cpu": 10,
        "portMappings": [
          {
            "hostPort": 80,
            "protocol": "tcp",
            "containerPort": 80
          },
          {
            "hostPort": 8888,
            "protocol": "tcp",
            "containerPort": 3000
          }
        ],
        "memory": 128,
        "essential": true
      }
    ]
  }
}